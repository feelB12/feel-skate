{% extends "profile/base.html.twig" %}

{% block metatitle %}
    Recherche User | Skateboard Social Club
{% endblock metatitle %}

{% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/hom.css') }}">
        <link rel="stylesheet" href="{{ asset('css/burger.css') }}">
        <link rel="stylesheet" href="{{ asset('css/search.css') }}">
{% endblock stylesheets %}

{% block title %}
    searching result
{% endblock title %}

{% block main %}
    {% for user in users %}
        <div class="detail top20">
            <div class="img-card">
                <h2> {{ user.title }}</h2>
                <h3> {{ user.description }}</h3>
            </div>
        </div>
    {% endfor %}
{% endblock main %}